<?xml version="1.0"?>
<layout>
    <adminhtml_citrusintegration_queue_index>
        <reference name="content">
            <block type="citrusintegration/adminhtml_citrusintegration_queue" name="citrus.integration.queue.grid.container"/>
        </reference>
    </adminhtml_citrusintegration_queue_index>

    <adminhtml_citrusintegration_queue_grid>
        <block type="citrusintegration/adminhtml_citrusintegration_queue_grid" name="citrus.integration.queue.grid" output="toHtml"/>
    </adminhtml_citrusintegration_queue_grid>

    <adminhtml_citrusintegration_ad_index>
        <reference name="content">
            <block type="citrusintegration/adminhtml_citrusintegration_ad" name="citrus.integration.ad.grid.container"/>
        </reference>
    </adminhtml_citrusintegration_ad_index>

    <adminhtml_citrusintegration_ad_grid>
        <block type="citrusintegration/adminhtml_citrusintegration_ad_grid" name="citrus.integration.ad.grid" output="toHtml"/>
    </adminhtml_citrusintegration_ad_grid>
    <citrus_top_container>
        <reference name="content">
            <block name="content_top" type="core/text_list" before="-" translate="label">
                <label>Main Column Top</label>
            </block>
        </reference>
    </citrus_top_container>
</layout>